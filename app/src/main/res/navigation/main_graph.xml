<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/searchFragment">

    <activity
        android:id="@+id/meaningActivity"
        android:name="com.test.skytest.screen.meaning.MeaningActivity"
        android:label="MeaningActivity">

        <argument
            android:name="meaningIds"
            app:argType="long[]" />
    </activity>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.test.skytest.screen.search.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search">
        <action
            android:id="@+id/action_searchFragment_to_meaningActivity"
            app:destination="@id/meaningActivity">
            <argument
                android:name="meaningIds"
                app:argType="long[]" />
        </action>
    </fragment>
</navigation>